@article{legendre_ChapterOrdinationReduced_1998,
  title = {Chapter 9 {{Ordination}} in Reduced Space},
  volume = {20},
  issn = {01678892},
  url = {https://www.sciencedirect.com/science/article/pii/S0167889298800614},
  doi = {10.1016/S0167-8892(98)80061-4},
  abstract = {Ordination in reduced space is often referred to as factor (or inertia) analysis because it is based on the extraction of the eigenvectors or factors of the association matrix. The chapter provides a description of the domains of application of the ordination methods, which include principal component analysis (PCA), principal coordinate analysis (PCoA), and nonmetric multidimensional scaling (MDS). PCA is a powerful technique for ordination in reduced space which is, however, limited to sets of quantitative descriptors. Results are also sensitive to the presence of double-zeros. PCoA (metric scaling) and nonmetric MDS, respectively project, in reduced space, the distances among objects computed using some appropriate association measure. Correspondence analysis is useful ordination technique for species presence/absence or abundance data. Correspondence analysis can also be conducted on contingency tables that compare two groups of descriptors. The analysis of the relationships among the descriptors of a multidimensional data matrix is carried out by factor analysis. The two types of factor analysis are exploratory factor analysis and confirmatory factor analysis.},
  number = {C},
  journaltitle = {Developments in Environmental Modelling},
  date = {1998-01},
