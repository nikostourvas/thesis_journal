  pages = {169-196},
  keywords = {bulked-segregant analysis,DNA markers,linkage map,marker-assisted selection,QTL analysis,QTL mapping,quantitative trait loci (QTLs)},
  author = {Collard, B. C. Y. and Jahufer, M. Z. Z. and Brouwer, J. B. and Pang, E. C. K.}
}

