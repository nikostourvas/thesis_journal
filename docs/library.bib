@article{aravanopoulos_GeneticMonitoringNatural_2011,
  langid = {english},
  title = {Genetic Monitoring in Natural Perennial Plant Populations},
  volume = {89},
  issn = {1916-2790, 1916-2804},
  url = {http://www.nrcresearchpress.com/doi/10.1139/b10-087},
  doi = {10.1139/b10-087},
  abstract = {Genetic monitoring, the quantification of temporal changes in population genetics and dynamics metrics generated by using appropriate parameters, constitutes a method with a prognostic value. Genetic monitoring has been recognized in several international agreements and documents, and can be an important tool for the protection of biodiversity. However, approaches developed so far for perennial plant species are rather cumbersome for practical use. It is proposed that perennial plant genetic monitoring should focus on keystone species of biological and economical importance, as well as rare or endangered species. In addition, genetic monitoring should concentrate on gene conservation units of such species, to be advanced in a dynamic gene conservation scheme. Three indicators are proposed for genetic monitoring based on a gene-ecological approach: natural selection, genetic drift, and a gene flow-mating system. These are evaluated based on three demographic (age and size class distribution, reproductive fitness, regeneration abundance) and four genetic (effective population size, allelic richness, latent genetic potential, outcrossing/actual inbreeding rate) parameters. Minimum sample sizes, critical levels of differences among parameters, and costs for temporal evaluation are proposed. The benefits of the immediate application of genetic monitoring are highlighted.},
  number = {2},
  journaltitle = {Botany},
  urldate = {2018-10-26},
  date = {2011-02},
  pages = {75-81},
  keywords = {genetic monitoring},
  author = {Aravanopoulos, F. A.}
}

