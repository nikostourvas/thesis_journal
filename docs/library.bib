@book{ivetic_GeneticDiversityForest_2016,
  title = {Genetic Diversity and Forest Reproductive Material - {{From}} Seed Source Selection to Planting},
  volume = {9},
  isbn = {1971-7458},
  url = {http://www.sisef.it/iforest/contents/?id=ifor1577-009},
  abstract = {How much of genetic diversity is desirable in mass production of forest$\backslash$nreproductive material? How mass production of forest reproductive$\backslash$nmaterial reduces genetic diversity? Relation between genetic diversity$\backslash$nand mass production of forest reproductive material is discussed in a$\backslash$nholistic manner. In industrial forest plantations, narrow genetic$\backslash$ndiversity is desirable and reproductive material is produced at clone$\backslash$nlevel. On the other hand, in conservation forestry a wide genetic$\backslash$ndiversity is imperative. Beside management goals, a desirable level ofŋenetic diversity is related to rotation cycle and ontogeny of tree$\backslash$nspecies. Risks of failure are lower in short rotations of fast growing$\backslash$nspecies. In production of slow growing species, managed in long$\backslash$nrotations, the reduction of genetic diversity increases the risk of$\backslash$nfailure due to causes unknown or unexpected at the time of planting.$\backslash$nThis risk is additionally increased in cases of seed transfer and in$\backslash$nconditions of climate change. Every step in production of forest$\backslash$nreproductive material, from collection to nursery production, has an$\backslash$neffect on genetic diversity mainly by directional selection and should$\backslash$nbe considered. This review revealed no consistent decrease of genetic$\backslash$ndiversity during forest reproductive material production and planting.},
  pagetotal = {801–812},
  number = {5},
  publisher = {{SISEF - Italian Society of Silviculture and Forest Ecology}},
  date = {2016},
  keywords = {resilience},
  author = {Ivetić, Vladan and Devetaković, Jovana and Nonić, Marina and Stanković, Dragica and Šijačić-Nikolić, Mirjana},
  doi = {10.3832/ifor1577-009}
}

