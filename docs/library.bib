@article{postolache_TranscriptomeGenomicMicrosatellite_2014,
  title = {Transcriptome versus {{Genomic Microsatellite Markers}}: {{Highly Informative Multiplexes}} for {{Genotyping Abies}} Alba {{Mill}}. and {{Congeneric Species}}},
  volume = {32},
  issn = {07359640},
  doi = {10.1007/s11105-013-0688-7},
  abstract = {The availability of high-resolution, cost-effective polymorphic genetic$\backslash$nmarkers displaying Mendelian inheritance is a prerequisite for$\backslash$nfine-scale population genetic analyses as well as informed conservation$\backslash$nand sustainable management. Silver fir (Abies alba Mill.) is a$\backslash$nwidespread European species of economic and ecological importance for$\backslash$nwhich genetic markers are needed but difficult to develop, as in most$\backslash$nconifer species. In this work, we introduce two sets of new multiplexed$\backslash$ntranscriptome-derived expressed sequence tag microsatellites (EST-simple$\backslash$nsequence repeats (SSRs)) which we compare to a set of multiplexedŋenomic microsatellites (gSSRs). For both marker types, transferability$\backslash$nwas tested in 17 congeneric taxa. A total of 16 new EST-SSRs and two newŋSSRs were developed. The EST-SSR multiplexes produced easily scorable$\backslash$namplification patterns that allow rapid and cost-effective genotyping at$\backslash$nlow-error rates, and include loci that display very low null allele$\backslash$nfrequencies. Generally, EST-SSRs displayed lower polymorphism and$\backslash$nfrequency of null alleles, but higher genetic differentiation among$\backslash$npopulations than gSSRs. Preliminary tests revealed that the EST-SSR$\backslash$nmarkers are highly transferable and polymorphic across Abies species.$\backslash$nThis study also confirmed that SSRs can be successfully developed using$\backslash$nnext-generation sequencing technology also in large genome species such$\backslash$nas conifers.},
  journaltitle = {Plant Molecular Biology Reporter},
  date = {2014},
  pages = {750--760},
  keywords = {Abies nSSR,methods,null allele estimation,Multiplex PCR},
  author = {Postolache, Dragos and Leonarduzzi, Cristina and Piotti, Andrea and Spanu, Ilaria and Roig, Anne and Fady, Bruno and Roschanski, Anna and Liepelt, Sascha and Vendramin, Giovanni Giuseppe}
