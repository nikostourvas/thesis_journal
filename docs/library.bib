@article{hauck_ReviewingDriversEcosystem_2015,
  langid = {english},
  title = {Reviewing Drivers of Ecosystem Change as Input for Environmental and Ecosystem Services Modelling},
  volume = {5},
  issn = {22126139},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S2212613915000045},
